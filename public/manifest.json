{
  "manifest_version": 3,
  "name": "TabbyNotes",
  "description": "Notes taking extention with tabs using local storage",
  "version": "4.0.0",
  "action": {
    "default_popup": "index.html",
    "default_title": "Open to take notes"
  },
  "icons": {
    "16": "kitty.png",
    "48": "kitty.png",
    "128": "kitty.png"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'sha256-rkTb15qUZNrKoaKNKjLcVI0tnem580klo1J1DbTsPBg='; object-src 'self'",
    "sandbox": "script-src 'self' 'sha256-rkTb15qUZNrKoaKNKjLcVI0tnem580klo1J1DbTsPBg='; object-src 'self'"
  },
  "permissions": ["storage"]
}