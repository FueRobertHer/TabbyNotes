{
  "manifest_version": 2,
  "name": "TabbyNotes",
  "description": "Notes taking extention with tabs using local storage",
  "version": "1.0.0",
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open to take notes"
  },
  "icons": {
    "16": "logo-small.png",
    "48": "logo-small.png",
    "128": "logo-small.png"
  },
  "content_security_policy": "script-src 'self' 'sha256-dyd6GvoE3kNaE6sxRtCPfZ+FKgs/+bqN15z7rHv4Ilc='; object-src 'self'",
  "permissions": []
}